{
    "name": "mpyw/laravel-file-errors",
    "description": "A tiny extension that reports validation error details about uploaded files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["file", "upload", "error", "laravel", "illuminate", "validation"],
    "autoload": {
        "psr-4": {
            "Mpyw\\LaravelFileErrors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\LaravelFileErrors\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "illuminate/http": "^5.6 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/validation": "^5.6 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "php-coveralls/php-coveralls": "^2.1"
    }
}

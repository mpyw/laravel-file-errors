{
    "name": "mpyw/laravel-file-errors",
    "description": "A tiny extension that reports validation error details about uploaded files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["file", "upload", "error", "laravel", "illuminate", "validation"],
    "autoload": {
        "psr-4": {
            "Mpyw\\LaravelFileErrors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\LaravelFileErrors\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/http": "^11.0 || ^12.0 || ^13.0",
        "illuminate/support": "^11.0 || ^12.0 || ^13.0",
        "illuminate/validation": "^11.0 || ^12.0 || ^13.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=9.0",
        "phpunit/phpunit": ">=11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
